{
  "name": "Doughnut",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "npm run watch",
    "test": "NODE_ENV=test electron-mocha --recursive --require babel-core/register",
    "rebuild": "electron-rebuild",
    "app": "NODE_ENV=development electron build/",
    "watch:main": "watchify -v -t [ babelify ] ./src/app/main.js --exclude electron --im --no-detect-globals --node -o ./build/main.js -d",
    "watch:view": "webpack --progress --colors --watch",
    "watch": "npm-run-all -p watch:*",
    "build:view": "elm make ./src/elm/Main.elm --output ./src/assets/elm.js && browserify -t [ babelify ] ./src/view/view.js -o ./src/assets/view.js",
    "release:js-main": "cross-env NODE_ENV=production browserify -t [ babelify ] ./src/app/main.js --exclude electron --im --no-detect-globals --node | uglifyjs -c warnings=false -m -d DEBUG=false > ./dist/src/assets/main.js",
    "release:js-view": "cross-env NODE_ENV=production browserify -t [ babelify ] ./src/view/view.js --exclude electron | uglifyjs -c warnings=false -m -d DEBUG=false > ./dist/src/assets/view.js",
    "release:clean": "rimraf ./dist/src",
    "release:copy": "cpx \"./src/**/!(*.js|*.styl|*.css|*.map)\" ./dist/src/",
    "release:build": "npm-run-all -s release:clean release:copy -p release:js-main release:js-view",
    "release:pack-osx": "cross-conf-env electron-packager ./dist/src Doughnut --out=dist/bin --download.cache=dist/cache --platform=darwin --arch=x64 --overwrite --asar --icon=res/icon.icns",
    "release:osx": "npm-run-all -s release:build release:pack-osx"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-conf-env": "^1.1.2",
    "css-loader": "^0.26.1",
    "electron": "^1.4.15",
    "electron-mocha": "^3.3.0",
    "electron-packager": "^8.5.1",
    "electron-rebuild": "^1.5.7",
    "elm-webpack-loader": "^4.2.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "watchify": "^3.9.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "feedparser": "^2.1.0",
    "jsonfile": "^2.4.0",
    "sequelize": "^3.30.2",
    "sqlite3": "^3.1.8",
    "umzug": "^1.11.0"
  }
}
