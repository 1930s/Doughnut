{
  "name": "Doughnut",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "npm run watch",
    "test": "NODE_ENV=test electron-mocha test/test.js --require babel-core/register",
    "rebuild": "electron-rebuild",
    "postinstall": "install-app-deps",
    "app": "NODE_ENV=development electron build/",
    "watch:main": "watchify -v -t [ babelify ] ./src/app/main.js --exclude electron --im --no-detect-globals --node -o ./compiled/main.js -d",
    "watch:view": "webpack --progress --colors --watch",
    "watch": "npm-run-all -p watch:*",
    "build:view": "elm make ./src/elm/Main.elm --output ./src/assets/elm.js && browserify -t [ babelify ] ./src/view/view.js -o ./src/assets/view.js",
    "release:main": "cross-env NODE_ENV=production browserify -t [ babelify ] ./src/app/main.js --exclude electron --im --no-detect-globals --node -o ./compiled/main.js",
    "release:view": "NODE_ENV=production webpack",
    "release:chmod": "chmod +x compiled/mac/mpv",
    "release:build": "npm-run-all release:view release:main release:chmod",
    "release:pack-osx": "electron-packager ./build Doughnut --out=dist/bin --download.cache=dist/cache --platform=darwin --arch=x64 --electron-version=1.4.15 --overwrite --asar --icon=res/icon.icns",
    "release:osx": "npm-run-all -s release:build release:pack-osx",
    "pack": "cd compiled && build --dir && cd ..",
    "dist": "cd compiled && build && cd .."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-conf-env": "^1.1.2",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.1",
    "electron": "^1.4.15",
    "electron-builder": "^14.5.2",
    "electron-mocha": "^3.3.0",
    "electron-packager": "^8.5.1",
    "electron-rebuild": "^1.5.7",
    "elm-webpack-loader": "^4.2.0",
    "envify": "^4.0.0",
    "expect.js": "^0.3.1",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "import-glob-loader": "^1.1.0",
    "md5-file": "^3.1.1",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "rss": "^1.2.2",
    "sass-loader": "^5.0.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "supertest": "^3.0.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.9.0",
    "webpack": "^2.2.1",
    "xml": "^1.0.1"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "crypto": "0.0.3",
    "feedparser": "^2.1.0",
    "jsonfile": "^2.4.0",
    "moment": "^2.17.1",
    "nan": "^2.5.1",
    "node-mpv": "^1.0.3",
    "portfinder": "^1.0.13",
    "request-progress": "^3.0.0",
    "request-promise": "^4.1.1",
    "sanitize-filename": "^1.6.1",
    "sequelize": "^3.30.2",
    "sqlite3": "^3.1.8",
    "umzug": "^1.11.0",
    "uuid": "^3.0.1"
  }
}
