{
  "name": "Doughnut",
  "version": "0.1.0",
  "description": "Podcast client (podcatcher) for Mac",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && electron .",
    "clean": "node ./bin/clean.js",
    "package": "node ./bin/package.js",
    "test": "standard --fix \"src/app/**/*.js\" && NODE_ENV=test electron-mocha test/test.js --require babel-core/register",
    "rebuild": "electron-rebuild",
    "build:main": "browserify -t [ babelify ] ./src/app/main.js --exclude electron --im --no-detect-globals --node -o ./build/main.js",
    "build:renderer": "webpack",
    "build:mpv": "chmod +x build/mac/mpv",
    "build": "npm-run-all -s build:renderer build:main build:mpv",
    "watch": "nodemon -V --exec \"npm run start\" --ext js,scss,elm --ignore build/ --ignore dist/",
    "prepackage": "uglifyjs --compress build/main.js -o build/main.js"
  },
  "repository": "https://github.com/CD1212/Doughnut.git",
  "keywords": [
    "electron",
    "electron-application",
    "podcast",
    "podcast-aggregator",
    "elm"
  ],
  "author": "Chris Dyer",
  "license": "GPLv3",
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-conf-env": "^1.1.2",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.1",
    "electron": "^1.6.3",
    "electron-builder": "^14.5.2",
    "electron-mocha": "^3.3.0",
    "electron-packager": "^8.5.1",
    "electron-rebuild": "^1.5.7",
    "elm-webpack-loader": "^4.2.0",
    "envify": "^4.0.0",
    "expect.js": "^0.3.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "import-glob-loader": "^1.1.0",
    "md5-file": "^3.1.1",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "rimraf": "^2.6.1",
    "rss": "^1.2.2",
    "sass-loader": "^5.0.1",
    "sinon": "^1.17.7",
    "standard": "^9.0.2",
    "style-loader": "^0.13.1",
    "supertest": "^3.0.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.9.0",
    "webpack": "^2.2.1",
    "xml": "^1.0.1"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "express": "^4.14.1",
    "deepmerge": "^1.3.2",
    "feedparser": "^2.1.0",
    "jquery": "^3.2.1",
    "jsonfile": "^2.4.0",
    "moment": "^2.17.1",
    "nan": "^2.5.1",
    "node-mpv": "^1.0.3",
    "portfinder": "^1.0.13",
    "request-progress": "^3.0.0",
    "request-promise": "^4.1.1",
    "sanitize-filename": "^1.6.1",
    "sequelize": "^3.30.2",
    "split.js": "^1.2.0",
    "sqlite3": "^3.1.8",
    "uuid": "^3.0.1"
  }
}
